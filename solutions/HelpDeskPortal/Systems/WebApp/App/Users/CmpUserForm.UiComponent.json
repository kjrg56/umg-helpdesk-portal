        "mandatory": true,
        "mode": "single",
        "name": "cmbDepto",
        "size": "half",
        "widget": "ComboBox"
      },
      {
        "binding": "user.username",
        "label": "Usuario",
        "mandatory": true,
        "name": "txtUserName",
        "size": "third",
        "widget": "TextBox"
      },
      {
        "label": "Contrase単a",
        "mandatory": true,
        "name": "txtPasswd1",
